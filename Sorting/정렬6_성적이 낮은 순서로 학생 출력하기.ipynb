    "n = int(input())\n",
    "\n",
    "array = []\n",
    "for i in range(n):\n",
    "    input_data = input().split()\n",
    "    array.append((input_data[0], int(input_data[1])))\n",
    "\n",
    "array = sorted(array, key = lambda student : student[1])\n",
    "\n",
    "for student in array:\n",
    "    print(student[0], end = ' ')"
